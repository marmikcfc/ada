{
    "config": {
        "model": "gpt-4o-mini",
        "openai_api_key_env": "OPENAI_API_KEY"
    },
    "servers": {
        "utilities": {
            "command": "python",
            "args": [
                "mcp_servers/utilities_server.py"
            ],
            "transport": "stdio"
        },
        "weather": {
            "url": "wss://server.smithery.ai/@turkyden/weather/ws?config={config_b64}&api_key={smithery_api_key}",
            "transport": "websocket"
        },
        "search_engine": {
            "url": "wss://server.smithery.ai/@turkyden/weather/ws?config={config_b64}&api_key={smithery_api_key}",
            "transport": "websocket"
        },
        "calculator_http": {
            "url": "http://localhost:3001/mcp",
            "transport": "http",
            "description": "HTTP-based calculator MCP server"
        }
    }
}